                 	ori R16, (1<<PD7)						;OR in the setup that we wish to have
                 	out DDRD, R16
                 
                 	ldi R16, 0b01101001						;Fast-pwm, non-inverting mode, no prescaling
                 	out TCCR2, R16							;
                 .ENDMACRO
                 
