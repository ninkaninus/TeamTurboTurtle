                 ;Define Types
                 .equ Comm_Type_SET = 0x55					;Used for setting a value or activating a command
                 .equ Comm_Type_GET = 0xAA					;Used for getting a value from the microcontroller
                 
                 ;Define Set Commands
                 .equ Comm_Command_Start = 0x10				;Start the car at a specific % of motor speed
                 .equ Comm_Command_Stop = 0x11				;Stop the car
                 
                 ;Define Get Commands
                 
                 
                 ;Implemetation of the required communication protocol
                 Comm_Received:
                 
0001e8 9100 007c 	lds R16, Comm_Received_Byte_Num			;What byte # did we receive
                 
0001ea 3001      	cpi R16, 0x01							;If it was the first
0001eb f019      	breq Comm_Received_Type					;Set the type
                 
0001ec 3002      	cpi R16, 0x02							;If it was the second
0001ed f041      	breq Comm_Received_Command				;Set the command
                 
                 	;Working under the assumption that if it was not the first or second byte, then it must be the third.
                 
0001ee c00e      	rjmp Comm_Received_Execute				;Go to the execution of the received telegram
                 
                 
                 ;Received a type
                 Comm_Received_Type:
                 
0001ef e002      	ldi R16, 0x02							;Set the counter to two
0001f0 9300 007c 	sts Comm_Received_Byte_Num, R16			;Store it
                 
0001f2 b10c      	in r16, UDR								;Store the received type in SRAM
0001f3 9300 007d 	sts Comm_Received_Byte_1, R16			;	
0001f5 9518      	reti									;Return from interrupt
                 
                 ;Received a command
                 Comm_Received_Command:
                 
0001f6 e003      	ldi R16, 0x03							;Set the counter to three
0001f7 9300 007c 	sts Comm_Received_Byte_Num, R16			;Store it
                 
0001f9 b10c      	in r16, UDR								;Store the received command in SRAM
0001fa 9300 007e 	sts Comm_Received_Byte_2, R16			;
0001fc 9518      	reti									;Return from interrupt
                 
                 ;Execute the telegram received
                 Comm_Received_Execute:
                 
0001fd b10c      	in r16, UDR								;Read in the received parameter
0001fe 9300 007f 	sts Comm_Received_Byte_3, R16			;Store it in SRAM
                 	
000200 e001      	ldi R16, 0x01							;Reset the counter
000201 9300 007c 	sts Comm_Received_Byte_Num, R16			;Store it in SRAM
                 
                 ;---------------------------------------------------------------------------------------------------------------------
                 ;Types
