                 ;I2C communication library 
                 
                  .equ I2C_ACK = 0x01	;Value for ACK used with I2C_Read
                  .equ I2C_NACK = 0x00	;Value for NACK used with I2C_Read
                 
                  .MACRO I2C_Init ;First argument is prescaler, second argument is TWBR
                 	;Set Prescaler
                 	ldi R16, @0
                 	out TWSR, R16
                 	
                 	;Set TWBR
                 	ldi R16, @1
                 	out TWBR, R16
                 
                 	;Enable I2C module
                 	ldi R16, (1<<TWEN)
                 	out TWCR, R16
                 
                 	nop ;Important delay to allow the module to start
                 
                  .ENDMACRO
                 
                 
                 ;This function sends a start command
                 I2C_Start:
                 	;Send Start condition
000114 ea04      	ldi r16, (1<<TWINT)|(1<<TWSTA)|(1<<TWEN) 
000115 bf06      	out TWCR, r16
                 
                 I2C_Start_Wait:	
                 	;Wait for the start condition transmit			
000116 b706      	in r16,TWCR
000117 ff07      	sbrs r16,TWINT
000118 cffd      	rjmp I2C_Start_Wait
000119 9508      ret
                 
                 
                 ;This function sends a stop command
                 I2C_Stop:
                 	;Send Stop condition
00011a e904      	ldi r16, (1<<TWINT)|(1<<TWSTO)|(1<<TWEN) 
00011b bf06      	out TWCR, r16
00011c 9508      ret
                 
                 
                 ;This function writes the data from R16, onto the I2C data line
                 I2C_Write_Call:
00011d b903      	out TWDR, r16
00011e e804      	ldi r16, (1<<TWINT) | (1<<TWEN)
00011f bf06      	out TWCR, r16
                 
                 I2C_Write_Call_Wait:
000120 b706      	in r16,TWCR
000121 ff07      	sbrs r16,TWINT
000122 cffd      	rjmp I2C_Write_Call_Wait
000123 9508      ret
                 
                 ;This is an easy to use macro of the write function
                 .MACRO I2C_Write
                 	ldi R16, @0
                 	call I2C_Write_Call
                 .ENDMACRO
                 
                 ;This function reads in data from the I2C data line and stores it in R16.
                 ;Remember to set R16 to either I2C_ACK or I2C_NACK before calling the function!
                 ;Failure to do so may result in unwanted behaviour.
                 I2C_Read_Call:
000124 fd00      	sbrc r16,0
000125 c002      	rjmp I2C_Read_Call_ACK
000126 e804      	ldi r16, (1<<TWINT) | (1<<TWEN)
000127 c001      	rjmp I2C_Read_Call_Continue
                 I2C_Read_Call_ACK:
000128 ec04      	ldi r16, (1<<TWINT) | (1<<TWEN) | (1<<TWEA)
                 I2C_Read_Call_Continue:
000129 bf06      	out TWCR, r16
                 I2C_Read_Call_Wait:
00012a b706      	in r16,TWCR
00012b ff07      	sbrs r16,TWINT
00012c cffd      	rjmp I2C_Read_Call_Wait
                 
00012d b103      	in R16, TWDR
00012e 9508      ret
                 
                 //Easy to use macro which takes I2C_Ack/I2C_Nack as argument.
                 .MACRO I2C_Read
                 	ldi R16, @0
                 	call I2C_Read_Call
                 .ENDMACRO
                 
                 
